    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "8.0.1",
    "jest": "28.1.3",
    "jest-cli": "28.1.3",
    "prettier": "^3.3.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3",
    "vite": "^7.2.4",
